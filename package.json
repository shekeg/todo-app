{
  "name": "todo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore src",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest",
